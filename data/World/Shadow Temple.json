            "Shadow Temple Boss Key Chest": "can_use(Dins_Fire)",
            "Shadow Temple Invisible Floormaster Chest": "True",
            "Shadow Temple Bongo Bongo Heart": "
                (Small_Key_Shadow_Temple, 5) and Boss_Key_Shadow_Temple and
