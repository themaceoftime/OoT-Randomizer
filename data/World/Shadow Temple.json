            "Bongo Bongo": "
                (Small_Key_Shadow_Temple, 5) and Boss_Key_Shadow_Temple and
                (Bow or can_use(Distant_Scarecrow) or (logic_shadow_statue and has_bombchus))",
            "Shadow Temple GS Triple Giant Pot": "True"
        }
